<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import com.alegd.materialfx.datatable.DataTable?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml">
    <Label text="test"/>
    <DataTable fx:id="dataTable" styleClass="card">
        <header>
            <JFXToolbar>
                <leftItems>
                    <Label>Header</Label>
                </leftItems>
                <rightItems>
                    <JFXButton text="Insert"/>
                </rightItems>
            </JFXToolbar>
        </header>
        <columns>
            <JFXTreeTableColumn fx:id="nameColumn" text="Name" styleClass="text-column"/>
            <JFXTreeTableColumn fx:id="ageColumn" text="Age" styleClass="numeric-column"/>
            <JFXTreeTableColumn fx:id="addressColumn" text="Address" styleClass="text-column"/>
        </columns>
    </DataTable>

    <padding>
        <Insets top="32" right="32" bottom="32" left="32"/>
    </padding>
</VBox>
